<article>
    <h1>
        <a href="<%- config.root %><%- item.path %>">
            <%- item.title %>
        </a>
    </h1>
  <p class="meta">
    <time datetime='<%= item.date.format(config.date_format) %>'>
        <svg class="icon icon-clock"><use xlink:href="#icon-clock"></use></svg>
        <%= item.date.format(config.date_format) %>
    </time>
    <%- partial('_partial/article-tags') %>
    <%- item.content %>
    <div class="divider clearfix">
    <% if(page.prev){ %>
     
        <a class="prev" href="<%- site.baseurl %>/<%- page.prev.path %>" title="<%= page.prev.title %>"> &#8678; <%= page.prev.title %></a>
      
    <% } %>
      
    <% if(page.next){ %>
      
        <a class="next" href="<%= site.baseurl %>/<%= page.next.path %>" title="<%= page.next.title %>">
        <%= page.next.title %> &#8680; </a>
     
    <% } %>
    </div>
    <div id="disqus_thread"></div>

    <noscript>Please enable JavaScript to view the <a href="http://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>
</article>
<script type="text/javascript">
  var disqus_shortname = 'janetwu';
  window.onload = function(){
    var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
    dsq.src = 'http://' + disqus_shortname + '.disqus.com/embed.js';
    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
    
  }
</script>