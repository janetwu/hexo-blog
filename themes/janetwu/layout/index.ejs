layout: index-layout
---
<% page.posts.each(function(item, index){ %>
    <%- partial('_partial/article-excerpt', {item: item, index:index}) %>
<% }); %>
<% if (page.total > 1){ %>
  <div class="pagination">
    <%- paginator({
      prev_text: "« Prev",
      next_text: "Next »"
    }) %>
  </div>
<% } %>